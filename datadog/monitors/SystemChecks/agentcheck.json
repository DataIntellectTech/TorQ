{
	"name": "Datadog Agent Check",
	"type": "service check",
	"query": "\"datadog.agent.up\".over(\"*\").by(\"host\").last(2).count_by_status()",
	"message": "Check if Agent is running. ",
	"tags": [
		"system"
	],
	"options": {
		"notify_audit": false,
		"locked": false,
		"timeout_h": 0,
		"silenced": {},
		"include_tags": true,
		"new_host_delay": 300,
		"notify_no_data": true,
		"renotify_interval": 0,
		"no_data_timeframe": 2,
		"thresholds": {
			"critical": 1,
			"warning": 1,
			"ok": 1
		}
	}
}
